{
    "$schema": "https://raw.githubusercontent.com/nicegui/nicegui/main/nicegui/tauri/tauri-conf-schema.json",
    "productName": "EisenApp",
    "version": "1.7.3",
    "identifier": "com.eisenhower.tasks-rust",
    "build": {
        "frontendDist": "../src"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "EisenApp",
                "width": 1400,
                "height": 900,
                "minWidth": 1000,
                "minHeight": 700,
                "decorations": true,
                "dragDropEnabled": false
            }
        ]
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "createUpdaterArtifacts": true,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "plugins": {
        "updater": {
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlBRDEyMENGOTcyQ0NBRDcKUldUWHlpeVh6eURSbWxxdWppOXM1T1JkWEtTa0lKWmpoaUQ3RE4xc05iaVBwOTc0TW9mdjcxSUgK",
            "endpoints": [
                "https://github.com/victorprouff/eisenapp/releases/latest/download/latest.json"
            ],
            "windows": {
                "installMode": "passive"
            }
        }
    }
}